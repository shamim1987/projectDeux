<h2>Weather it's here or there, it's everywhere.</h2>
<div class="post">
<button type="button" name="article[new]"> <a href="/new-article">New Post!</a></button>
</div>

<ul>
  <%allArticles.forEach(function(article){%>
     <ul>
    <li><strong>Title :</strong><%=article.title%></li>
    <li><%=article.body %></li>
    <li><a href ="/new-comment">Click here to judge this post</a></li>
    <li><strong>Comments :</strong><%=article.comment %></li>
    </ul>
    <%})%>
</ul>

<a href="/logout">Logout</a>
